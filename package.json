    "scripts": {
        "dev:docs": "node scripts/dev.docs.js",
        "dev:web": "node scripts/dev.web.js",
        "build:web": "node scripts/build.web.js",
        "dev": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true node scripts/dev.js",
        "db": "knex --knexfile db/config.js",
        "test:unit": "jest --verbose",
        "build": "node --max_old_space_size=3052 scripts/build.js",
        "build:dir": "cross-env BUILD_TARGET=dir CSC_IDENTITY_AUTO_DISCOVERY=false node --max_old_space_size=4096 scripts/build.js",
        "build:lite": "cross-env BUILD_TARGET=lite CSC_IDENTITY_AUTO_DISCOVERY=false node scripts/build.js",
        "build:production": "cross-env BUILD_TARGET=production CSC_IDENTITY_AUTO_DISCOVERY=false node --max_old_space_size=4096 scripts/build.js",
        "lint": "eslint --ext .ts,.vue,.js src scripts",
        "lint:fix": "npm run lint -- --fix",
        "postinstall": "node ./scripts/dev.install.js && electron-builder install-app-deps",
        "server": "cross-env ARCHITECTURE=bs node --inspect -r tsconfig-paths/register src/dist-server/server/index.js",
        "server:pre": "tsc -p src",
        "server:dev": "node src/server/dev/index.js",
        "server:run": "cross-env ARCHITECTURE=bs node -r tsconfig-paths/register src/dist-server/server/index.js",
        "server:prod": "pm2 start src/server/prod/index.js",
        "server:rmTmp": "node src/server/script/rmTmp.js"
    },
